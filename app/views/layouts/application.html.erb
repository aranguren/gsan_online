<!DOCTYPE html>
<html>
  <head>
    <title>GSAN Online</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <base href="/">
  </head>
  <body ng-app="gsan" data-cadastro-url="<%= CADASTRO_URL %>">
    <%= render "shared/header" %>

    <div class="row">
      <div class="col-sm-3 col-md-2 sidebar">
        <%= render "shared/nav" %>
      </div>
      <div class="col-sm-9 col-md-10 main">
        <div ng-view></div>
      </div>
    </div>

    <footer>
      <div class="infos">
        <div class="pull-left"><h2>GSAN - Online</h2></div>
        <div class="pull-right"><%= image_tag "cidades.png" %></div>
      </div>
    </footer>
  </body>
</html>
